    use_frameworks!
    
    pod 'Revmob', :podspec => "https://raw.githubusercontent.com/DmitriiP/Revmob-Ios-Pod/master/Revmob.podspec"
    pod 'SICAds', :git => 'https://github.com/admincaf/SICAds_library.git'
    
    pod 'Alamofire'
    pod 'AlamofireImage'
    
    pod 'LTHPasscodeViewController'
    pod 'DropDown'
    pod 'Zip'
end
